@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<IEnumerable<Umbraco.Commerce.DemoStore.Models.IProductPage>>

@if (Model.Any())
{
    <div class="flex flex-wrap -mx-3">
        @foreach (var product in Model)
        {
            @await Html.PartialAsync("ProductListItem", product);
        }
    </div>
}